{
  "name": "n8n-nodes-http-trace-exporter",
  "version": "1.0.0",
  "description": "An n8n community node to make HTTP requests and export traces for observability.",
  "author": "nanull0x",
  "license": "MIT",
  "keywords": [
    "n8n",
    "node",
    "http",
    "tracing",
    "observability"
  ],
  "main": "dist/nodes/HttpTraceExporter.node.js",

  "scripts": {
    "build": "tsc",
    "dev": "n8n-node-dev",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },

  "dependencies": {
    "@opentelemetry/exporter-trace-otlp-http": "^0.53.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@opentelemetry/sdk-trace-base": "^1.26.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "axios": "^1.7.7",

    "n8n-core": "^1.45.0",
    "n8n-workflow": "^1.45.0"
  },

  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "n8n-node-dev": "^1.7.0"
  },

  "overrides": {
    "form-data": "4.0.4",
    "tmp": "0.2.4"
  },

  "n8n": {
    "credentials": [
      "TraceExporterApi.credentials.ts"
    ],
    "nodes": [
      "HttpTraceExporter.node.ts"
    ]
  }
}
